{
  "name": "promptproof-cli",
  "version": "0.1.0",
  "description": "CLI for PromptProof - Deterministic LLM testing",
  "bin": {
    "promptproof": "./dist/index.js"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "cd ../evaluator && npm run build",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@promptproof/evaluator": "file:../evaluator",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "files": [
    "dist",
    "templates"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "llm",
    "testing",
    "cli",
    "prompt",
    "ai"
  ]
}
